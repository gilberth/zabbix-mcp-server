{
  "name": "@thelord/zabbix-mcp-server",
  "version": "1.0.4",
  "description": "A comprehensive MCP server for Zabbix integration with TypeScript support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "zabbix-mcp-server": "dist/index.js"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    ".env.example"
  ],
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "tsx src/test.ts",
    "prepublishOnly": "npm run build",
    "postinstall": "echo 'Zabbix MCP Server installed successfully! Run: npx @thelord/zabbix-mcp-server'"
  },
  "keywords": [
    "zabbix",
    "mcp",
    "model-context-protocol",
    "monitoring",
    "api",
    "server",
    "typescript",
    "trae-ide"
  ],
  "author": {
    "name": "Gilbert H",
    "email": "your-email@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thelord/zabbix-mcp-server.git"
  },
  "homepage": "https://github.com/thelord/zabbix-mcp-server#readme",
  "bugs": {
    "url": "https://github.com/thelord/zabbix-mcp-server/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
